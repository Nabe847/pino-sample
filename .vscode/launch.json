    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach",
            "address": "localhost",
            "port": 9229,
            "request": "attach",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        },
        {
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}",
            "request": "attach",
            "skipFiles": ["<node_internals>/**"],
            "type": "pwa-node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "プログラムの起動",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}\\dist\\index.js",
            "outFiles": ["${workspaceFolder}/**/*.js"],
            "outputCapture": "std"
        }
    ]
}
